{
  "version": "fbep-AMD-20y1202",
  "pwm_transition_value": 70,
  "pwm_boost_value": 100,
  "sample_interval_ms": 1000,
  "fan_recovery_time": 1,
  "boost": {
    "fan_fail": true,
    "sensor_fail": true
  },
  "sensor_valid_check": {
    "(.*)pax": {
      "attribute": {
        "type" : "power_status"
      }
    },
    "(.*)gpu": {
      "attribute": {
        "type" : "power_status"
      }
    },
    "(.*)hbm": {
      "attribute": {
        "type" : "power_status"
      }
    },
    "(.*)vicor": {
      "attribute": {
        "type" : "power_status"
      }
    }
  },
  "watchdog": false,
  "fanpower": true,
  "min_rpm": 1500,
  "profiles": {
    "linear_inlet": {
      "read_source" : {
        "util" : "/usr/local/bin/sensor-util"
      },
      "type": "linear",
      "positive_hysteresis": 1,
      "negative_hysteresis": 1,
      "data": [
        [23, 10],
        [28, 15],
        [32, 20],
        [37, 25],
        [39, 45],
        [42, 65],
        [45, 80]
      ]
    },
    "linear_switch_remote": {
      "read_source" : {
        "util" : "/usr/local/bin/sensor-util"
      },
      "type": "linear",
      "positive_hysteresis": 0,
      "negative_hysteresis": 0,
      "data": [
        [60, 10],
        [65, 15],
        [70, 20],
        [75, 25],
        [80, 30],
        [90, 60]
      ]
    },
    "linear_gpu_spot": {
      "read_source" : {
        "util" : "/usr/local/bin/sensor-util"
      },
      "type": "linear",
      "positive_hysteresis": 2,
      "negative_hysteresis": 2,
      "data": [
        [60, 10],
        [65, 15],
        [70, 20],
        [75, 25],
        [80, 30],
        [85, 35]
      ]
    },
    "pid_gpu_spot": {
      "read_source" : {
	"util" : "/usr/local/bin/sensor-util"
      },
      "type": "pid",
      "setpoint": 90,
      "positive_hysteresis": 2,
      "negative_hysteresis": 1,
      "kp": -1.25,
      "ki": -0.035,
      "kd": 0.0
    },
    "linear_gpu_mem": {
      "read_source" : {
        "util" : "/usr/local/bin/sensor-util"
      },
      "type": "linear",
      "positive_hysteresis": 2,
      "negative_hysteresis": 2,
      "data": [
        [55, 10],
        [60, 15],
        [65, 20],
        [70, 25],
        [75, 30]
      ]
    },
    "pid_gpu_mem": {
      "read_source" : {
	"util" : "/usr/local/bin/sensor-util"
      },
      "type": "pid",
      "setpoint": 80,
      "positive_hysteresis": 2,
      "negative_hysteresis": 1,
      "kp": -1.25,
      "ki": -0.035,
      "kd": 0.0
    },
    "linear_vicor": {
      "read_source" : {
        "util" : "/usr/local/bin/sensor-util"
      },
      "type": "linear",
      "positive_hysteresis": 0,
      "negative_hysteresis": 0,
      "data": [
        [60, 10],
        [65, 15],
        [70, 20],
        [75, 25],
        [80, 30],
        [90, 60]
      ]
    }
  },
  "fans": {
    "0": {
      "label" : "Fan 0 Inlet",
      "read_source" : {
        "util" : "/usr/local/bin/fan-util --get 0"
      },
      "write_source" : {
        "util" : "/usr/local/bin/fan-util --set %d 0"
      }
    },
    "1": {
      "label" : "Fan 0 Outlet",
      "read_source" : {
        "util" : "/usr/local/bin/fan-util --get 1"
      },
      "write_source" : {
        "util" : "/usr/local/bin/fan-util --set %d 0"
      }
    },
    "2": {
      "label" : "Fan 1 Inlet",
      "read_source" : {
        "util" : "/usr/local/bin/fan-util --get 2"
      },
      "write_source" : {
        "util" : "/usr/local/bin/fan-util --set %d 1"
      }
    },
    "3": {
      "label" : "Fan 1 Outlet",
      "read_source" : {
        "util" : "/usr/local/bin/fan-util --get 3"
      },
      "write_source" : {
        "util" : "/usr/local/bin/fan-util --set %d 1"
      }
    },
    "4": {
      "label" : "Fan 2 Inlet",
      "read_source" : {
        "util" : "/usr/local/bin/fan-util --get 4"
      },
      "write_source" : {
        "util" : "/usr/local/bin/fan-util --set %d 2"
      }
    },
    "5": {
      "label" : "Fan 2 Outlet",
      "read_source" : {
        "util" : "/usr/local/bin/fan-util --get 5"
      },
      "write_source" : {
        "util" : "/usr/local/bin/fan-util --set %d 2"
      }
    },
    "6": {
      "label" : "Fan 3 Inlet",
      "read_source" : {
        "util" : "/usr/local/bin/fan-util --get 6"
      },
      "write_source" : {
        "util" : "/usr/local/bin/fan-util --set %d 3"
      }
    },
    "7": {
      "label" : "Fan 3 Outlet",
      "read_source" : {
        "util" : "/usr/local/bin/fan-util --get 7"
      },
      "write_source" : {
        "util" : "/usr/local/bin/fan-util --set %d 3"
      }
    }
  },
  "zones": {
    "zone_1": {
      "pwm_output": [0, 2, 4, 6],
      "expr_file": "zone1_AMD.fsc"
    }
  }
}
